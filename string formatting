def print_formatted(number):
    # Calculate the width based on the binary representation of 'number'
    # bin(number) returns something like '0b10001', so we slice from index 2
    width = len(bin(number)[2:])

    for i in range(1, number + 1):
        # Convert i to each format and remove the prefix
        dec = str(i)
        octa = oct(i)[2:]
        hexa = hex(i)[2:].upper() # Hex must be capitalized
        bina = bin(i)[2:]

        # Use rjust to right-align each value based on the calculated width
        print(f"{dec.rjust(width)} {octa.rjust(width)} {hexa.rjust(width)} {bina.rjust(width)}")

if __name__ == '__main__':
    n = int(input())
    print_formatted(n)
