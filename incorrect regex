import re
T = int(input())
for _ in range(T):
    s = input()
    try:
        re.compile(s)
        # extra validation for invalid repeats like *+, ++, ?*, etc.
        if re.search(r'(\*\+|\+\*|\?\+|\+\?|\*\*|\+\+|\?\?)', s):
            print(False)
        else:
            print(True)
    except re.error:
        print(False)
